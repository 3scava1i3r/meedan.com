    "gulp-concat": "^2.5.2",
    "gulp-debug": "~2.0.1",
    "gulp-dust": "^2.0.0",
    "gulp-favicons": "~1.0.9",
    "gulp-file-include": "~0.9.0",
    "gulp-html-validator": "0.0.5",
    "gulp-image-resize": "~0.6.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-jshint": "~1.9.2",
